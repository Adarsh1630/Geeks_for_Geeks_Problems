
class Solution {
    // Function to reverse words in a given string.
    String reverseWords(String str) {
        // code here
        
        String a[]=str.split("\\.");
        
        String str2="";
        
        for(int i=a.length-1;i>=0;i--)
        {
            if(i==0)
            str2=str2 + a[i];
            else
            str2=str2+a[i]+".";
        }
        return str2;
    }
}
